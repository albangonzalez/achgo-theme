{
	"$schema": "https://schemas.wp.org/wp/6.8/theme.json",
	"settings": {
		"color": {
			"palette": [
				{
					"color": "#ffffff",
					"name": "Base",
					"slug": "base"
				},
				{
					"color": "#275674",
					"name": "Contrast",
					"slug": "contrast"
				},
				{
					"color": "#6393a5",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#6393a5",
					"name": "Blue",
					"slug": "blue"
				},
				{
					"color": "#ecf3f6",
					"name": "Light Blue",
					"slug": "light-blue"
				},
				{
					"color": "#f45c24",
					"name": "Orange",
					"slug": "orange"
				}
			],
			"gradient": [
				{
					"gradient": "linear-gradient(45deg, #275674, #6393A5)",
					"name": "ACHGO",
					"slug": "achgo"
				}
			]
		},
		"appearanceTools": true,
		"layout": {
			"contentSize": "840px",
			"wideSize": "1100px"
		},
		"spacing": {
			"units": [ "%", "px", "em", "rem", "vh", "vw" ]
		},
		"typography": {
			"fontFamilies": [
				{
					"name": "Poppins",
					"slug": "poppins",
					"fontFamily": "Poppins, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Poppins",
							"fontWeight": "300",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Poppins-Light.woff2" ]
						}
					]
				},
				{
					"name": "Montserrat",
					"slug": "montserrat",
					"fontFamily": "Montserrat, sans-serif",
					"fontFace": [
						{
							"fontFamily": "Montserrat",
							"fontWeight": "100 900",
							"fontStyle": "normal",
							"fontStretch": "normal",
							"src": [ "file:./assets/fonts/Montserrat-Variable.woff2" ]
						}
					]
				}
			]
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"color": {
			"text": "var:preset|color|contrast",
			"background": "var:preset|color|base"
		},
		"typography": {
			"fontFamily": "var:preset|font-family|poppins",
			"fontWeight": "300"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var:preset|font-family|montserrat",
					"fontWeight": "700"

				}
			},
			"h2": {
				"typography": {
					"fontSize": "var:preset|font-size|large"
				}
			},
			"link": {
				"color": {
					"text": "var:preset|color|primary"
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3
}
